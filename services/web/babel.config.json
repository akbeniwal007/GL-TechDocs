{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        // This version must be aligned with the `core-js` version in `package.json`
        "corejs": { "version": 3.6 }
      }
    ],
    ["@babel/react", { "runtime": "automatic" }],
    "@babel/typescript"
  ],
  "plugins": ["angularjs-annotate", "macros"]
}
